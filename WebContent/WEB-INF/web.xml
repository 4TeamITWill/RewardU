<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TP</display-name>
  
  <servlet>
  	<servlet-name>MemberFrontController</servlet-name>
  	<servlet-class>member.action.MemberFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MemberFrontController</servlet-name>
  	<url-pattern>*.me</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>MessageFrontController</servlet-name>
  	<servlet-class>message.action.MessageFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MessageFrontController</servlet-name>
  	<url-pattern>*.message</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>admin</servlet-name>
    <servlet-class>admin.adFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>*.ad</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>myPage</servlet-name>
    <servlet-class>myPage.myFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myPage</servlet-name>
    <url-pattern>*.my</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>notice</servlet-name>
    <servlet-class>notice.action.NoticeFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>notice</servlet-name>
    <url-pattern>*.no</url-pattern>
  </servlet-mapping>

  <servlet>
  	<servlet-name>RewardController</servlet-name>
  	<servlet-class>reward.action.RewardController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RewardController</servlet-name>
  	<url-pattern>*.fu</url-pattern>
  </servlet-mapping>

  <servlet>
  	<servlet-name>AboutRewardFrontController</servlet-name>
  	<servlet-class>aboutReward.action.AboutRewardFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AboutRewardFrontController</servlet-name>
  	<url-pattern>*.ar</url-pattern>
  </servlet-mapping>

	<servlet>
		<servlet-name>NewsFrontController</servlet-name>
		<servlet-class>news.action.NewsFrontController</servlet-class>
	</servlet>
  	<servlet-mapping>
  		<servlet-name>NewsFrontController</servlet-name>
  		<url-pattern>*.news</url-pattern>
  	</servlet-mapping>

  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  
  <resource-ref>
  	<description>Connection</description>
  	<res-ref-name>jdbc/jspbeginner</res-ref-name>
  	<res-type>javax.sql.DataSource</res-type>
  	<res-auth>Container</res-auth>
  </resource-ref>
  
</web-app>